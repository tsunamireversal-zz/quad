. $QUAD_LOCATION/aliases/shell/colors
. $QUAD_LOCATION/aliases/shell/sys
. $QUAD_LOCATION/aliases/shell/files
. $QUAD_LOCATION/aliases/shell/dirs
. $QUAD_LOCATION/aliases/shell/net
. $QUAD_LOCATION/aliases/shell/hw
. $QUAD_LOCATION/aliases/quadonly/quick
. $QUAD_LOCATION/aliases/quadonly/error_handling
. $QUAD_LOCATION/aliases/shell/common_mistakes
. $QUAD_LOCATION/aliases/shell/forceload
. $QUAD_LOCATION/aliases/shell/obsolete
. $QUAD_LOCATION/aliases/shell/translations
. $QUAD_LOCATION/fix/app
. $QUAD_LOCATION/aliases/shell/specific_debian
. $QUAD_LOCATION/apps/shell/apps
. $QUAD_LOCATION/apps/gui/apps
. $QUAD_LOCATION/aliases/gui/mate/handlers

#Load specific shell aliases depending on user shell.
if [[ "$SHELL" == "/bin/bash" ]];then
  source $QUAD_LOCATION/aliases/shell/bash_specific
elif [[ "$SHELL" == "/bin/csh" ]];then
  source $QUAD_LOCATION/aliases/shell/csh_specific
elif [[ "$SHELL" == "/bin/tcsh" ]];then
  source $QUAD_LOCATION/aliases/shell/tcsh_specific
elif [[ "$SHELL" == "/bin/zsh" ]];then
  source $QUAD_LOCATION/aliases/shell/zsh_specific
fi


#PROFILES
. $QUAD_LOCATION/profiles/.profile_prompt
